{
  "name": "apollo-angular-ssr",
  "version": "0.0.0",
  "author": "Kamil Kisiela <kamil.kisiela@gmail.com>",
  "scripts": {
    "start": "npm run ssr",
    "ssr": "npm run build:ssr && npm run serve:ssr",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "ts-node -P tsconfig.server.json server.ts",
    "build:client-and-server-bundles": "ng build --prod && ng run apollo-angular-ssr:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress",
    "serve": "ng serve"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.4",
    "@angular/common": "6.1.4",
    "@angular/compiler": "6.1.4",
    "@angular/core": "6.1.4",
    "@angular/forms": "6.1.4",
    "@angular/http": "6.1.4",
    "@angular/platform-browser": "6.1.4",
    "@angular/platform-browser-dynamic": "6.1.4",
    "@angular/router": "6.1.4",
    "apollo-angular": "^1.2.0",
    "apollo-angular-link-http": "1.2.0",
    "apollo-cache-inmemory": "1.2.9",
    "apollo-client": "2.4.1",
    "core-js": "2.5.7",
    "graphql": "^0.13.2",
    "graphql-tag": "2.9.2",
    "rxjs": "6.2.2",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.7.5",
    "@angular/cli": "6.1.5",
    "@angular/compiler-cli": "6.1.4",
    "@angular/language-service": "6.1.4",
    "@angular/platform-server": "6.1.4",
    "@nguniversal/express-engine": "6.1.0",
    "@nguniversal/module-map-ngfactory-loader": "6.1.0",
    "@types/node": "8.9.4",
    "ts-loader": "4.5.0",
    "ts-node": "5.0.1",
    "tslint": "5.9.1",
    "typescript": "2.9.2",
    "webpack-command": "0.4.1"
  }
}
